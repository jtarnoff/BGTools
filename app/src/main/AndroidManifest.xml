<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jtarnoff.bgtools" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".NavigationActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jtarnoff.bgtools.TestActivity"
            android:label="@string/title_activity_test"
            android:parentActivityName="com.jtarnoff.bgtools.NavigationActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jtarnoff.bgtools.NavigationActivity" />
        </activity>
        <activity
            android:name="com.jtarnoff.bgtools.sevenWonders.SevenWondersActivity"
            android:label="@string/title_activity_test"
            android:parentActivityName="com.jtarnoff.bgtools.NavigationActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jtarnoff.bgtools.NavigationActivity" />
        </activity>
    </application>

</manifest>
